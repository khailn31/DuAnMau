<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:layout_margin="8dp">
    <RelativeLayout
        android:layout_marginLeft="20dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <TextView
        android:textStyle="bold"
        android:id="@+id/tv_id_s"
        android:layout_width="90dp"
        android:layout_height="30dp"/>
    <TextView
        android:layout_marginLeft="30dp"
        android:layout_toRightOf="@+id/tv_id_s"
        android:id="@+id/tv_name_s"
        android:layout_width="200dp"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/price"
        android:layout_below="@+id/tv_id_s"
        android:text="Giá thuê :"
        android:layout_width="90dp"
        android:layout_height="20dp"/>
    <TextView
        android:layout_marginLeft="30dp"
        android:text="32323232323232"
        android:id="@+id/tv_price_s"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_below="@+id/tv_id_s"
        android:layout_toRightOf="@+id/price"/>
    </RelativeLayout>

    <ImageView

        android:layout_marginRight="70dp"
        android:layout_gravity="end|center_vertical"
        android:id="@+id/update_s"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_baseline_edit_24"/>
    <ImageView
        android:layout_marginRight="20dp"
        android:layout_gravity="end|center_vertical"
        android:id="@+id/del_s"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_baseline_delete_24"/>
</androidx.cardview.widget.CardView>